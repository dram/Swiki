| ddir privDict |ddir _ shelf storage dir directoryNamed: 'default'.(ddir fileExists: 'security.xml')	ifTrue: [privDict _ Dictionary new.		(shelf formatPrivAddress: 'security' request: request response: response) do: [:arr | privDict at: (arr at: 1) put: (arr at: 3)].		shelf modulesAt: 'adminSecurity' put: (SwikiSecurityModule fromXml: ((ddir readOnlyFileNamed: 'security.xml') contentsOfEntireFile) withDict: privDict)]	ifFalse: [shelf modulesAt: 'adminSecurity' put: nil]