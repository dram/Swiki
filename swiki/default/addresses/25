| id user |id _ request fieldsAsNumberKey: 'id' ifAbsent: [nil].user _ (shelf modulesAt: 'users') userAtId: id ifAbsent: [nil].user	ifNil: [response at: 'headerStatus' put: #notFound.		shelf formatShelfTemplate: 'notFound' request: request response: response]	ifNotNil: [		request settingsAt: 'user' put: user.		shelf formatShelfTemplate: 'user' request: request response: response]