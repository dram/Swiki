| securityModule realm |(securityModule _ shelf modulesAt: 'adminSecurity' ifAbsent: [nil])	ifNil: [nil]	ifNotNil: [(securityModule process: request response: response shelf: shelf)		ifTrue: [nil]		ifFalse: ["Unauthorized"			realm _ request referenceShelf: shelf address: 'admin/*'.			response at: 'headerStatus' put: #unauthorized.			response at: 'contentType' put: (MIMEDocument contentTypePlainText).			response at: 'authenticate' put: ('Basic realm="', realm, '"').			'Unauthorized for: ', realm]]