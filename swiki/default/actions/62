(book rawModules at: 'security' ifAbsent: [nil])	ifNil: [(book hasPrivAddress: 'security')		ifTrue: [shelf formatBookTemplate: 'createSecurity' request: request response: response book: book]		ifFalse: [shelf formatBookTemplate: 'noSecurity' request: request response: response book: book]]	ifNotNil: [shelf formatBookTemplate: 'editSecurity' request: request response: response book: book]