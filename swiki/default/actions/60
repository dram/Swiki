"inheritable, visible, logging, accessible""name=aString description=aString value=aString Or aBoolean values=String, aOrderedCollection, Boolean, nil"| values return |values _ request settingsAt: 'setup'.return _ WriteStream on: String new.return	nextPutAll: '<table border=0 cellpadding=5 cellspacing=0>';	nextPutAll: String crlf.values do: [:value | request settingsAt: 'show' put: value.	(value isKindOf: String)		ifTrue: [return			nextPutAll: '<tr><td colspan=2 bgcolor="#cccccc"><b>';			nextPutAll: value;			nextPutAll: '</b>'] 		ifFalse: [			return				nextPutAll: '<tr><td><i>';				nextPutAll: (value at: 1);				nextPutAll: '</i><br>(';				nextPutAll: (value at: 2);				nextPutAll: ')</td><td>'.			return nextPutAll: ((value at: 4) caseOf: {				[nil]->[shelf formatShelfAction: 'showNil' request: request response: response].				[String]->[shelf formatShelfAction: 'showString' request: request response: response].				[Boolean]->[shelf formatShelfAction: 'showBoolean' request: request response: response]}				otherwise: [shelf formatShelfAction: 'showCollection' request: request response: response])].	return		nextPutAll: '</td></tr>';		nextPutAll: String crlf].return	nextPutAll: '</table>';	contents