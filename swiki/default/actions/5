| sort return |sort _ shelf books asSortedCollection: [:a :b | (a name asLowercase) < (b name asLowercase)].return _ WriteStream on: String new.return	nextPutAll: '<ul>';	nextPutAll: String crlf.sort do: [:book |	return		nextPutAll: '<li><a href="';		nextPutAll: (request referenceShelf: shelf address: 'admin/setup?book=');		nextPutAll: book name;		nextPutAll: '">';		nextPutAll: book name;		nextPutAll: '</a>'.	(book settingsIncludes: 'description') ifTrue: [return		nextPutAll: ': ';		nextPutAll: (book settingsAt: 'description')].	return		nextPutAll: String crlf].return	nextPutAll: '</ul>';	contents