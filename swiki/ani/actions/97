| bookName fs leftBanner rightBanner |String streamContents: [:stream |	bookName _ (book pages at: 1) showStrictName.	fs _ book modulesAt: 'uploadServer'.	leftBanner _ book formatBookAction: 'leftBanner' request: request response: response shelf: shelf.	rightBanner _ book formatBookAction: 'rightBanner' request: request response: response shelf: shelf.	"Put in left image"	stream nextPutAll: (leftBanner		ifNil: ['<h1>&nbsp;', bookName, '</h1>']		ifNotNil: [fs			referenceFile: (leftBanner at: 2)			onDir: (leftBanner at: 1)			options: (Dictionary new				at: 'border' put: 0;				at: 'alt' put: bookName;				yourself)]).	rightBanner ifNotNil: [stream		nextPutAll: '</td><td class="banner" align=right valign=bottom>';		nextPutAll: (fs			referenceFile: (rightBanner at: 2)			onDir: (rightBanner at: 1)			options: (Dictionary new				at: 'border' put: 0;				at: 'alt' put: '';				at: 'align' put: 'right';				yourself))]]