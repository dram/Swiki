| accessControl groups groupsAll groupsUsers |accessControl _ page accessControl.groups _ book groups.groupsAll _ (groups select: [:group | group beSignedIn not]) asSortedCollection: [:a :b | a name < b name].groupsUsers _ (groups select: [:group | group beSignedIn]) asSortedCollection: [:a :b | a name < b name].String streamContents: [:stream |	stream		nextPutAll: '<small><table border=0 cellspacing=0 cellpadding=0>';		nextPutAll: String crlf;		nextPutAll: '<tr><td><a class="internal" href="';		nextPutAll: (request referenceShelf: shelf book: book address: 'manageGroups');		nextPutAll: '" target="_new">User Group</a></td><td><u>Access Level</u></td></tr>';		nextPutAll: String crlf.	accessControl printALChoicesForAllOn: stream.	groupsAll do: [:group | accessControl printALChoicesForGroup: group on: stream].	accessControl printALChoicesForUsersOn: stream.	groupsUsers do: [:group | accessControl printALChoicesForGroup: group on: stream].	stream		nextPutAll: '</table></small>';		nextPutAll: String crlf;		nextPutAll: '<script type="text/javascript">';		nextPutAll: String crlf;		nextPutAll: '<!--';		nextPutAll: String crlf.	groupsAll do: [:group | stream		nextPutAll: 'addNoUserGroup(document.editForm.ac';		nextPutAll: group showId;		nextPutAll: ', 0);';		nextPutAll: String crlf].	groupsUsers do: [:group | stream		nextPutAll: 'addNoUserGroup(document.editForm.ac';		nextPutAll: group showId;		nextPutAll: ', 0);';		nextPutAll: String crlf].	stream		nextPutAll: 'initialize();';		nextPutAll: String crlf;		nextPutAll: '//-->';		nextPutAll: String crlf;		nextPutAll: '</script>']