String streamContents: [:stream |	"Add lock symbol if page is not editable"	(book aniAllowEditPage: page)		ifFalse: [stream nextPutAll: ((book modulesAt: 'colorScheme') button: 'lockedit')].	"Add lock symbol if page is not viewable"	(book aniAllowViewPage: page)		ifFalse: [stream nextPutAll: ((book modulesAt: 'colorScheme') button: 'lockview')]]