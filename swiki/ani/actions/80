| parent file directory |(directory _ (book modulesAt: 'uploads') directoryNamed: (page id asString) ifAbsent: [nil])	ifNil: [(parent _ page parent)		ifNil: [book formatBookAction: 'styleTextBook' request: request response: response shelf: shelf]		ifNotNil: [book formatPageAction: 'styleText' request: request response: response shelf: shelf page: parent]]	ifNotNil: [(file _ directory fileRefsCacheAt: 'style.css' ifAbsent: [nil])		ifNil: [(parent _ page parent)			ifNil: [book formatBookAction: 'styleTextBook' request: request response: response shelf: shelf]			ifNotNil: [book formatPageAction: 'styleText' request: request response: response shelf: shelf page: parent]]		ifNotNil: [TextFormatter crToCrlf: (directory readOnlyFile: file last) contentsOfEntireFile]]