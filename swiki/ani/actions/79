| directory file |directory _ book modulesAt: 'uploads'.(file _ directory fileRefsCacheAt: 'style.css' ifAbsent: [nil])	ifNil: ["Find it in the color scheme"		directory _ (book modulesAt: 'colorScheme') directory.		(file _ directory fileRefsCacheAt: 'style.css' ifAbsent: [nil])			ifNil: ['']			ifNotNil: [TextFormatter crToCrlf: (directory readOnlyFile: file last) contentsOfEntireFile]]	ifNotNil: [TextFormatter crToCrlf: (directory readOnlyFile: file last) contentsOfEntireFile]