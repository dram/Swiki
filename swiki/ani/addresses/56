| user save check |user _ request security.(user isOwnerOfBook: book) ifFalse: [	save _ false.	request groupIds do: [:groupId |		check _ book name, '-', groupId asString.		(user settingsIncludes: check) ifFalse: [			user settingsAt: check put: true.			save _ true]].	save ifTrue: [user save]]."Reset cookie passwords"request cookiePasswords isEmpty ifFalse: [	request clearCookiePasswords]