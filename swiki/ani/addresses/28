"Similar to Common Log Format"| raw ipAddress logFile date time stamp |(logFile _ book modulesAt: 'logFile') ifNotNil: [	raw _ request raw.	ipAddress _ (ipAddress _ raw remoteAddress)		ifNil: ['not available']		ifNotNil: [NetNameResolver stringFromAddress: ipAddress].	logFile nextPutAll: (String streamContents: [:aStream |		aStream			nextPutAll: ipAddress.		request security ifNotNil: [aStream			nextPutAll: ' (';			nextPutAll: (request security idAsString);			nextPutAll: ')'].		aStream			nextPutAll: '	-	-	['.		"GMT TimeStamp"		stamp _ raw timestamp.		date _ Date fromDays: stamp // 86400.		time _ Time fromSeconds: stamp \\ 86400.		date printOn: aStream format: #(1 2 3 $/ 2 1).		aStream nextPut: $:.		time print24: true on: aStream.		aStream			nextPutAll: ' -0400]	'; "Eastern Standard Time"			nextPutAll: raw url asString; tab;			nextPutAll: (response at: 'headerStatus' ifAbsent: [#ok]) asString; tab;			nextPutAll: (response at: 'content') size asString; tab;			nextPutAll: (request raw header at: 'user-agent' ifAbsent: ['']); cr])]