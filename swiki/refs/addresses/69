(book rawSettingsIncludes: 'uploadsLimit')	ifTrue: [		book modulesAt: 'uploadsSize' put: ((book settingsAt: 'uploadsLimit') caseOf: {			['3 Mb']->[3000000].			['10 Mb']->[10000000].			['20 Mb']->[20000000].			['50 Mb']->[50000000].			['100 Mb']->[100000000].			['200 Mb']->[200000000].			['500 Mb']->[500000000].			['1 Gb']->[1000000000].			['2 Gb']->[2000000000].			['5 Gb']->[5000000000].			['10 Gb']->[10000000000]}			otherwise: [0])]	ifFalse: [		book rawModulesRemove: 'uploadsSize']