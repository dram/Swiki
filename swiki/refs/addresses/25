page isText	ifTrue: [book formatPageTemplate: ('edit', page lockStateString) request: request response: response shelf: shelf page: page]	ifFalse: ["Forward to default locations, since projects can always be edited"		response at: 'headerStatus' put: #tempMoved.		response at: 'location' put: (request referenceShelf: shelf book: book page: page).		book formatPageTemplate: 'editProject' request: request response: response shelf: shelf page: page]