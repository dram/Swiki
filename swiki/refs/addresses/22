| version nextVersion |(version _ page version: (request fieldsAsNumberKey: 'id' ifAbsent: [0]))	ifNil: [response at: 'headerStatus' put: #notFound.		book formatBookTemplate: 'addressNotFound' request: request response: response shelf: shelf]	ifNotNil: [		request settingsAt: 'version' put: version.		nextVersion _ page version: ((request fieldsAsNumberKey: 'id') + 1).		nextVersion ifNil: [nextVersion _ page].		request settingsAt: 'nextVersion' put: nextVersion.		(page settingsAt: 'isText' ifAbsent: [true])			ifTrue: [book formatPageTemplate: 'version' request: request response: response shelf: shelf page: page]		ifFalse: [book formatPageTemplate: 'versionProject' request: request response: response shelf: shelf page: page]]