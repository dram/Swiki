(request fieldsHasKey: 'password')	ifTrue: ["Plug-in Message"		response at: 'contentType' put: (MIMEDocument contentTypePlainText).		(page settingsAt: 'lock' ifAbsent: [false])			ifTrue: [(page passwordMatches: request response: response shelf: shelf book: book)				ifTrue: ['true']				ifFalse: ['false']]			ifFalse: ['true']]	ifFalse: [response at: 'headerStatus' put: #notFound.		book formatBookTemplate: 'addressNotFound' request: request response: response shelf: shelf]