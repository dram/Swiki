(book setup at: 'logging' ifAbsent: ['none']) caseOf: {	['none']->[book modulesAt: 'logFile' put: nil].	['individual']->[book modulesAt: 'logFile' put: 		((book storage dir fileNamed: 'log.txt') setToEnd; yourself)].	['site']->[book modulesAt: 'logFile' put: (shelf modulesAt: 'logFile')]}	otherwise: [shelf error: 'That is not a valid value for logging']