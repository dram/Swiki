(page settingsAt: 'isText' ifAbsent: [true])	ifTrue: [(page settingsIncludes: 'template')		ifTrue: [book formatPageTemplate: ((page settingsAt: 'template'), '-show') request: request response: response shelf: shelf page: page]		ifFalse: [book formatPageTemplate: 'showText' request: request response: response shelf: shelf page: page]]	ifFalse: [book formatPageTemplate: 'showProject' request: request response: response shelf: shelf page: page]