(request fieldsKey: 'lock' ifAbsent: ['unlocked']) caseOf: {	['unlocked']->[book formatPageTemplate: 'requestSecurityUnlocked' request: request response: response shelf: shelf page: page].	['locked']->[book formatPageTemplate: 'requestSecurityLocked' request: request response: response shelf: shelf page: page].	['open']->[book formatPageTemplate: 'requestSecurityOpen' request: request response: response shelf: shelf page: page]}	otherwise: ['']